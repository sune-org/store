[{"id":"e9r2j2m","name":"Sune Forums (indev)","pinned":false,"avatar":"data:image/webp;base64,","url":"gh://sune-org/store/forum.sune","updatedAt":1757538480610,"settings":{"model":"","temperature":"","top_p":"","top_k":"","frequency_penalty":"","repetition_penalty":"","min_p":"","top_a":"","verbosity":"","reasoning_effort":"default","system_prompt":"","html":"<!-- Sune Forums v1.1.0 -->\n<div class=\"mx-auto max-w-2xl font-sans text-stone-800\" x-data=\"suneForums()\" x-init=\"init()\">\n  <style>\n    .sune-forums-accent-bg{background-color:#E8BCAE}.sune-forums-accent-bg:hover{background-color:#e3af9b}.sune-forums-accent-focus:focus-visible{--tw-ring-color:#E8BCAE;--tw-ring-offset-shadow:var(--tw-ring-inset) 0 0 0 var(--tw-ring-offset-width) var(--tw-ring-offset-color);--tw-ring-shadow:var(--tw-ring-inset) 0 0 0 calc(2px + var(--tw-ring-offset-width)) var(--tw-ring-color);box-shadow:var(--tw-ring-offset-shadow),var(--tw-ring-shadow),var(--tw-shadow, 0 0 #0000)}\n    .sune-forums-input:focus{border-color:#E8BCAE;--tw-ring-color:#E8BCAE;box-shadow:var(--tw-ring-offset-shadow,0 0 #0000),var(--tw-ring-shadow,0 0 #0000),var(--tw-shadow,0 0 #0000)}\n  </style>\n\n  <!-- Header -->\n  <header class=\"flex items-center justify-between mb-4 sm:mb-6 pb-3 border-b border-stone-200\">\n    <div class=\"flex items-center gap-3\">\n      <button x-show=\"view !== 'list'\" @click.prevent=\"goToList()\" class=\"h-9 w-9 flex items-center justify-center rounded-full hover:bg-stone-100 transition-colors\" aria-label=\"Back\">\n        <i data-lucide=\"arrow-left\" class=\"h-5 w-5 text-stone-600\"></i>\n      </button>\n      <h1 class=\"text-xl font-bold text-stone-900\" x-text=\"view === 'list' ? 'Sune Forums' : (view === 'new_post' ? 'Create a Post' : (currentPost ? currentPost.title : '...'))\"></h1>\n    </div>\n    <div class=\"flex items-center gap-2\">\n      <span class=\"text-xs text-stone-400\">v1.1.0</span>\n      <button x-show=\"view === 'list'\" @click.prevent=\"view = 'new_post'\" class=\"px-3 py-1.5 text-sm font-semibold sune-forums-accent-bg text-stone-900 rounded-lg sune-forums-accent-focus transition-colors\">\n        New Post\n      </button>\n    </div>\n  </header>\n\n  <!-- Error State -->\n  <div x-show=\"error\" x-transition class=\"p-3 mb-4 bg-red-50 text-red-800 border border-red-200 rounded-lg\" x-text=\"error\"></div>\n\n  <!-- Main Content Area -->\n  <main class=\"relative\">\n    <!-- Loading Overlay -->\n    <div x-show=\"isLoading\" x-transition:enter=\"transition-opacity duration-300\" x-transition:leave=\"transition-opacity duration-300\" class=\"absolute inset-0 z-10 flex items-center justify-center pt-10\">\n      <i data-lucide=\"loader-circle\" class=\"h-6 w-6 animate-spin text-stone-400\"></i>\n    </div>\n    \n    <!-- Views -->\n    <div :class=\"{'opacity-50 blur-sm pointer-events-none': isLoading}\">\n      <!-- Posts List -->\n      <div x-show=\"view === 'list'\" x-transition>\n        <div x-show=\"!isLoading && posts.length === 0\" class=\"text-center py-12\">\n          <p class=\"text-stone-500\">No posts yet. Be the first!</p>\n        </div>\n        <div class=\"space-y-2\">\n          <template x-for=\"post in posts\" :key=\"post.id\">\n            <a href=\"#\" @click.prevent=\"selectPost(post.id)\" class=\"block p-3 rounded-lg hover:bg-stone-50 transition-colors duration-200\">\n              <p class=\"font-semibold text-stone-900\" x-text=\"post.title\"></p>\n              <p class=\"text-xs text-stone-500 mt-1\">By <span class=\"font-medium\" x-text=\"post.author_name\"></span> &middot; <span x-text=\"formatDate(post.created_at)\"></span></p>\n            </a>\n          </template>\n        </div>\n      </div>\n\n      <!-- New Post Form -->\n      <div x-show=\"view === 'new_post'\" x-transition class=\"p-1\">\n        <form @submit.prevent=\"submitPost()\" class=\"space-y-4\">\n          <input type=\"text\" x-model=\"newPost.title\" placeholder=\"Post Title\" required class=\"w-full p-2 border border-stone-300 rounded-lg sune-forums-input transition\">\n          <textarea x-model=\"newPost.content\" placeholder=\"What's on your mind, Master?\" required rows=\"8\" @input=\"autoResize($el)\" class=\"w-full p-2 border border-stone-300 rounded-lg sune-forums-input transition resize-none\"></textarea>\n          <div class=\"flex justify-end gap-2\">\n            <button type=\"button\" @click=\"goToList()\" class=\"px-4 py-2 font-semibold bg-stone-100 text-stone-800 rounded-lg hover:bg-stone-200 transition-colors\">Cancel</button>\n            <button type=\"submit\" class=\"px-4 py-2 font-semibold sune-forums-accent-bg text-stone-900 rounded-lg sune-forums-accent-focus transition-colors\">Post</button>\n          </div>\n        </form>\n      </div>\n\n      <!-- Single Post & Comments -->\n      <div x-show=\"view === 'post' && currentPost\" x-transition class=\"space-y-6\">\n        <article class=\"p-4 bg-stone-50/70 border border-stone-200/80 rounded-xl\">\n          <p class=\"text-xs text-stone-500 mb-2\">By <span class=\"font-medium\" x-text=\"currentPost.author_name\"></span> &middot; <span x-text=\"formatDate(currentPost.created_at)\"></span></p>\n          <div class=\"markdown-body\" data-post-content x-html=\"render(currentPost.content)\"></div>\n        </article>\n        <section class=\"space-y-4\">\n          <h2 class=\"text-lg font-bold text-stone-900 border-b border-stone-200 pb-2\">Comments (<span x-text=\"currentPost.comments.length\"></span>)</h2>\n          <div x-show=\"currentPost.comments.length === 0\" class=\"text-sm text-stone-500 text-center py-4\">No comments yet.</div>\n          <div class=\"space-y-3\">\n            <template x-for=\"comment in currentPost.comments\" :key=\"comment.id\">\n              <div class=\"p-3 bg-white border border-stone-200 rounded-lg\">\n                <p class=\"text-xs text-stone-500 mb-1.5\"><span class=\"font-semibold text-stone-700\" x-text=\"comment.author_name\"></span> &middot; <span x-text=\"formatDate(comment.created_at)\"></span></p>\n                <div class=\"markdown-body\" :data-comment-id=\"comment.id\" x-html=\"render(comment.content)\"></div>\n              </div>\n            </template>\n          </div>\n          <form @submit.prevent=\"submitComment()\" class=\"pt-4 flex items-start gap-2\">\n            <textarea x-model=\"newComment\" placeholder=\"Add a comment...\" required rows=\"1\" @input=\"autoResize($el)\" class=\"flex-1 resize-none p-2 bg-white border border-stone-300 rounded-lg sune-forums-input transition\"></textarea>\n            <button type=\"submit\" class=\"shrink-0 px-4 py-2 font-semibold sune-forums-accent-bg text-stone-900 rounded-lg sune-forums-accent-focus transition-colors\">Reply</button>\n          </form>\n        </section>\n      </div>\n    </div>\n  </main>\n</div>\n<script>\nfunction suneForums() {\n  const k=window.SUNE.id+'-forums';\n  return {\n    view:'list',posts:[],currentPost:null,isLoading:!1,error:'',\n    newPost:{title:'',content:''},newComment:'',\n    md:window.markdownit({html:!1,breaks:!0,linkify:!0}),\n    init(){\n      const c=JSON.parse(localStorage.getItem(k))||{};\n      this.newPost=c.newPost||{title:'',content:''};\n      this.newComment=c.newComment||'';\n      this.$watch('newPost',()=>this.saveState());this.$watch('newComment',()=>this.saveState());\n      this.$watch('currentPost',(p)=>{if(p&&this.view==='post')this.$nextTick(()=>this.enhanceAllMarkdown())});\n      this.fetchPosts()\n    },\n    saveState(){localStorage.setItem(k,JSON.stringify({newPost:this.newPost,newComment:this.newComment}))},\n    async query(sql,p=[]){\n      this.isLoading=!0;this.error='';\n      try{\n        const r=await fetch('https://d1p.awww.workers.dev',{method:'POST',headers:{'Content-Type':'application/json'},body:JSON.stringify({query:sql,params:p})});\n        if(!r.ok){const d=await r.json().catch(()=>null);throw new Error(d?.error||`HTTP Error ${r.status}`)}\n        const d=await r.json();if(d.error)throw new Error(d.details||d.error);return d.results\n      }catch(e){this.error=e.message;console.error(e);return null}finally{this.isLoading=!1}\n    },\n    async fetchPosts(){const r=await this.query(\"SELECT id,author_name,title,created_at FROM posts ORDER BY created_at DESC\");if(r)this.posts=r},\n    async selectPost(id){\n      this.view='post';this.currentPost=null;\n      const [p,c]=await Promise.all([this.query(\"SELECT * FROM posts WHERE id = ?\", [id]),this.query(\"SELECT * FROM comments WHERE post_id = ? ORDER BY created_at ASC\", [id])]);\n      if(p&&p.length)this.currentPost={...p[0],comments:c||[]}\n    },\n    async submitPost(){\n      const{title:t,content:c}=this.newPost;if(!t.trim()||!c.trim())return;\n      const a=window.USER?.name||'Anonymous';\n      await this.query(\"INSERT INTO posts(author_name,title,content)VALUES(?,?,?)\",[a,t.trim(),c.trim()]);\n      if(!this.error){this.newPost={title:'',content:''};this.goToList()}\n    },\n    async submitComment(){\n      const c=this.newComment.trim();if(!c||!this.currentPost)return;\n      const a=window.USER?.name||'Anonymous';\n      await this.query(\"INSERT INTO comments(post_id,author_name,content)VALUES(?,?,?)\",[this.currentPost.id,a,c]);\n      if(!this.error){this.newComment='';await this.selectPost(this.currentPost.id)}\n    },\n    goToList(){this.view='list';this.currentPost=null;this.error='';this.fetchPosts()},\n    formatDate(s){if(!s)return'';try{return new Date(s.replace(' ','T')+'Z').toLocaleDateString(undefined,{month:'short',day:'numeric'})}catch{return s}},\n    render(t){return this.md.render(t||'')},\n    autoResize(el){el.style.height='auto';el.style.height=(el.scrollHeight+2)+'px'},\n    enhanceAllMarkdown(){\n      if(!window.enhanceCodeBlocks)return;\n      const postEl=this.$root.querySelector('[data-post-content]');\n      if(postEl)window.enhanceCodeBlocks(postEl,!0);\n      this.$root.querySelectorAll('[data-comment-id]').forEach(el=>window.enhanceCodeBlocks(el,!0))\n    }\n  }\n}\n</script>\n","extension_html":"<sune src='https://raw.githubusercontent.com/sune-org/store/refs/heads/main/sync.sune' private></sune>","hide_composer":true},"storage":{}}]